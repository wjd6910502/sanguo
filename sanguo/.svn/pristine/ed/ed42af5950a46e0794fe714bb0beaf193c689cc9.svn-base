function OnCommand_DebugCommand(player, role, arg, others)
	player:Log("OnCommand_DebugCommand, "..DumpTable(arg))

	if arg.typ=="levelup" then
		--level up
		role._roledata._status._level = role._roledata._status._level+arg.count1
