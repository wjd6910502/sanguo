<procedure name="changecardnumber" connection="auth0" operate="replaceA">
	<parameter name="number"      sql-type="varchar(32)" java-type="java.lang.String" in="true" out="false" />
	BEGIN TRAN
	DECLARE @number_old varchar(32)
	SELECT @number_old = number from cardnew WHERE number like substring(@number,1,10)+'%'
	IF @@rowcount = 1
	BEGIN
		IF NOT @number_old = @number
		BEGIN
			UPDATE cardnew SET number = @number WHERE number = @number_old
			IF @@rowcount = 1
			BEGIN
				COMMIT TRAN
				return 0
			END
			ELSE
			BEGIN
				COMMIT TRAN
				return -1
			END
		END
		ELSE
		BEGIN
			COMMIT TRAN
			return -2
		END
	END
	ELSE
	BEGIN
		COMMIT TRAN
		return -3
	END
	COMMIT TRAN
</procedure>


