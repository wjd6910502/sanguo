<procedure name="statusedinfo" connection="auth0" operate="replaceA">
	<parameter name="type"  sql-type="integer"   java-type="java.lang.Integer" in="true" out="false" />
	BEGIN TRAN
	DECLARE @addall integer
	DECLARE @consumedall integer
	DECLARE @exchgtomonthall integer
	DECLARE @remainall integer
	DECLARE @presentall integer
	DECLARE @addtoday integer
	DECLARE @addtodaymoney integer
	DECLARE @exchgtomonthtoday integer
	DECLARE @difftoday integer
	DECLARE @consumedtoday integer
	DECLARE @consumedbymonthtoday integer

	DECLARE @player_transinmonth integer
	DECLARE @player_transnoadd integer
	DECLARE @player_trans integer

	DECLARE @payremainall integer
	DECLARE @paynetremainall integer
	DECLARE @paypresentall integer
	DECLARE @netconsumedtoday integer
	DECLARE @netconsumedbymonthtoday integer
	DECLARE @monthremainall integer
	DECLARE @monthnetremainall integer

	DECLARE @remainall_last integer
	DECLARE @presentall_last integer
	DECLARE @payremainall_last integer
	DECLARE @paynetremainall_last integer
	DECLARE @paypresentall_last integer

	DECLARE @now datetime
	DECLARE @last datetime
	SELECT @now = getdate()
	SELECT @last = dateadd(day,-1,@now)

	SELECT @addall=convert(int,sum(adduppoint*1.0)/3600) from function at isolation read uncommitted

	SELECT @exchgtomonthall=convert(int,sum(usepoint*1.0)/3600) from monthbill

	SELECT @remainall=convert(int,sum(time*1.0)/3600),@presentall=10*count(time) from point where NOT aid=0 at isolation read uncommitted

	SELECT @consumedall = @addall + @presentall - @remainall - @exchgtomonthall

	SELECT @addtoday=convert(int,sum(addpoint*1.0)/3600),@addtodaymoney=sum(money) from agentbill where uid>32 and usedate>@last and @now>usedate at isolation read uncommitted
	SELECT @exchgtomonthtoday=convert(int,sum(usepoint*1.0)/3600) from monthbill where usedate>@last
	SELECT @difftoday = @addtoday - @exchgtomonthtoday

	SELECT @consumedbymonthtoday=sum(datediff(hour,@last,enddate)) from point where enddate>@last and @now>enddate at isolation read uncommitted
	SELECT @consumedbymonthtoday=@consumedbymonthtoday+sum(24) from point where enddate>@now at isolation read uncommitted

	SELECT @player_transinmonth=count(distinct buyer) from translog where date>dateadd(day,-30,@now) at isolation read uncommitted
	SELECT @player_transnoadd=count(distinct buyer) from translog,agentbill where translog.buyer=agentbill.uid and agentbill.usedate>dateadd(day,-30,@now)
	SELECT @player_trans = @player_transinmonth - @player_transnoadd

	SELECT @remainall_last=remainall,@presentall_last=presentall,@payremainall_last=payremainall,@paynetremainall_last=paynetremainall,@paypresentall_last=paypresentall from usedinfo where statdate>dateadd(hour,-25,@now) and dateadd(hour,-23,@now) > statdate
	SELECT @consumedtoday = @remainall_last - @remainall + @presentall - @presentall_last + @difftoday



	DELETE FROM billinfo
	INSERT into billinfo SELECT a.uid,a.aid,sum(a.money),sum(a.addpoint) from agentbill a where a.uid>32 group by a.uid,a.aid
	DELETE FROM transbillinfo
	INSERT into transbillinfo SELECT t.buyer,t.aid,sum(t.point)/90,sum(t.point),0 from translog t where t.status=1 group by t.buyer,t.aid
	UPDATE transbillinfo set t.flags=1 from billinfo b,transbillinfo t where b.uid=t.uid and b.aid=t.aid
	INSERT into billinfo select t.uid,t.aid,t.addupmoney,t.adduppoint from transbillinfo t where NOT t.flags=1
	UPDATE billinfo set b.adduppoint=b.adduppoint+t.adduppoint,b.addupmoney=b.addupmoney+t.addupmoney from billinfo b,transbillinfo t where b.uid=t.uid and b.aid=t.aid and t.flags=1
	DELETE FROM transbillinfo
	INSERT into transbillinfo select t.seller,t.aid,-sum(t.point)/90,-sum(t.point),0 from translog t where t.status=1 group by t.seller,t.aid
	UPDATE transbillinfo set t.flags=1 from billinfo b,transbillinfo t where b.uid=t.uid and b.aid=t.aid
	INSERT into billinfo select t.uid,t.aid,t.addupmoney,t.adduppoint from transbillinfo t where NOT t.flags=1
	UPDATE billinfo set b.adduppoint=b.adduppoint+t.adduppoint,b.addupmoney=b.addupmoney+t.addupmoney from billinfo b,transbillinfo t where b.uid=t.uid and b.aid=t.aid and t.flags=1

	SELECT @payremainall=convert(int,sum(p.time*1.0)/3600) from point p,billinfo b where p.uid=b.uid and p.aid=b.aid and NOT p.aid=0 and b.adduppoint>0
	SELECT @paynetremainall=convert(int,sum(p.time*1.0)/3600) from point p,billinfo b where p.uid=b.uid and p.aid=b.aid and NOT p.aid=0 and b.adduppoint>0 and b.addupmoney*90>p.time
	SELECT @paynetremainall=@paynetremainall+convert(int,sum(b.addupmoney*90.0)/3600) from point p,billinfo b where p.uid=b.uid and p.aid=b.aid and NOT p.aid=0 and b.adduppoint>0 and p.time>b.addupmoney*90
	SELECT @paypresentall=10*count(*) from billinfo
	SELECT @netconsumedtoday = @paynetremainall_last - @paynetremainall + @difftoday
	SELECT @consumedtoday = @payremainall_last - @payremainall + @paypresentall - @paypresentall_last + @difftoday

	UPDATE consumeadjust set adjustenddate=dateadd(second,2592*((a.time-b.addupmoney*90)/522),recorddate) from consumeadjust a,billinfo b where a.uid=b.uid and a.aid=b.aid and a.adjustenddate=NULL and a.time>b.addupmoney*90
	SELECT @netconsumedbymonthtoday=sum(datediff(hour,@last,adjustenddate)) from consumeadjust where adjustenddate>@last and @now>adjustenddate at isolation read uncommitted
	SELECT @netconsumedbymonthtoday=@netconsumedbymonthtoday+sum(24) from consumeadjust where adjustenddate>@now at isolation read uncommitted
	SELECT @netconsumedbymonthtoday = @consumedbymonthtoday-@netconsumedbymonthtoday
	SELECT @monthremainall=sum(datediff(hour,@now,enddate)) from point where enddate>@now at isolation read uncommitted
	SELECT @monthnetremainall=sum(datediff(hour,@now,adjustenddate)) from consumeadjust where adjustenddate>@now at isolation read uncommitted
	SELECT @monthnetremainall = @monthremainall-@monthnetremainall
	DELETE from consumeadjust where @last>adjustenddate

	INSERT INTO usedinfo (statdate,addall,consumedall,exchgtomonthall,remainall,presentall,addtoday,addtodaymoney,exchgtomonthtoday,difftoday,consumedtoday,consumedbymonthtoday,player_transinmonth,player_transnoadd,player_trans,payremainall,paynetremainall,paypresentall,netconsumedtoday,netconsumedbymonthtoday,monthremainall,monthnetremainall) VALUES (@now,@addall,@consumedall,@exchgtomonthall,@remainall,@presentall,@addtoday,@addtodaymoney,@exchgtomonthtoday,@difftoday,@consumedtoday,@consumedbymonthtoday,@player_transinmonth,@player_transnoadd,@player_trans,@payremainall,@paynetremainall,@paypresentall,@netconsumedtoday,@netconsumedbymonthtoday,@monthremainall,@monthnetremainall)

	SELECT * from usedinfo where statdate = @now
	COMMIT TRAN
</procedure>

<procedure name="statonlineinfo" connection="auth0" operate="replaceA">
	<parameter name="type"  sql-type="integer"   java-type="java.lang.Integer" in="true" out="false" />
	BEGIN TRAN
		DECLARE @account_pay integer
		DECLARE @account_pay_now integer
		DECLARE @account_month integer
		DECLARE @account_month_now integer
		DECLARE @online_total integer
		DECLARE @online_pay integer
		DECLARE @online_month integer
		DECLARE @online_freesvr integer
		DECLARE @online_freesvr_payed integer

		DECLARE @now datetime
		SELECt @now = getdate()

		SELECT @account_pay=count(uid) FROM function where addupmoney>0 at isolation read uncommitted
		SELECT @account_pay_now=count(distinct point.uid) FROM point,agentbill where point.uid=agentbill.uid and point.aid=agentbill.aid and agentbill.money>0 and (point.time>0 or point.enddate>getdate()) at isolation read uncommitted
		SELECT @account_month=count(distinct uid) FROM monthbill
		SELECT @account_month_now=count(*) FROM point where enddate > @now at isolation read uncommitted
		SELECT @online_total=count(*) FROM point where NOT zoneid = NULL at isolation read uncommitted
		SELECT @online_pay=count(*) FROM point,function where point.uid = function.uid and NOT point.accountstart = NULL and NOT point.accountstart = '' and NOT point.zoneid = NULL and NOT point.zoneid = 0 and function.addupmoney > 0 and NOT point.aid = 0 at isolation read uncommitted
		SELECT @online_month=count(*) FROM point where NOT accountstart = NULL and NOT accountstart = '' and NOT zoneid = NULL and NOT zoneid = 0 and enddate > @now at isolation read uncommitted
		SELECT @online_freesvr=count(*) FROM point where point.aid >= 9 and 10 >= point.aid and NOT point.zoneid = NULL at isolation read uncommitted
		SELECT @online_freesvr_payed=count(*) FROM point,function where point.uid = function.uid and point.aid >= 9 and 10 >= point.aid and NOT point.zoneid = NULL and function.addupmoney > 0 at isolation read uncommitted

		INSERT INTO onlineinfo (account_pay,account_pay_now,account_month,account_month_now,online_total,online_pay,online_month,online_freesvr,online_freesvr_payed,statdate) VALUES (@account_pay,@account_pay_now,@account_month,@account_month_now,@online_total,@online_pay,@online_month,@online_freesvr,@online_freesvr_payed,@now)
	COMMIT TRAN
</procedure>

 
