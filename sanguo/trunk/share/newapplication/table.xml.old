<procedure name="accounting" connection="auth0" operate="replaceA">
	<parameter name="uid"      sql-type="integer"  java-type="java.lang.Integer"     in="true"  out="false" />
	<parameter name="aid"      sql-type="integer"  java-type="java.lang.Integer"     in="true"  out="false" />
	<parameter name="type"     sql-type="integer"  java-type="java.lang.Integer"     in="true"  out="false" />
	BEGIN TRAN
	DECLARE @remain       integer
	DECLARE @enddate      datetime
	DECLARE @accountstart datetime
	DECLARE @now          datetime
	DECLARE @timeused     integer
	DECLARE @timefree     integer
	DECLARE @timeminus    integer
	SELECT @now = getdate()
	SELECT @timeused = 0
	SELECT @timefree = 0
	SELECT @timeminus = 0
	SELECT @remain = time, @enddate = enddate, @accountstart = ISNULL(accountstart,@now) FROM point WHERE @uid = uid AND @aid = aid
	IF @@rowcount = 1
	BEGIN
		IF '2005-01-01' > @accountstart	SELECT @accountstart = @now
	END
	ELSE
	BEGIN
		SELECT @remain = 36000
		SELECT @enddate = NULL
		SELECT @accountstart = @now
		IF @type = 1
			INSERT INTO point (uid,aid,time,zoneid,zonelocalid,accountstart,lastlogin,enddate) VALUES (@uid,@aid,@remain, 0, 0, '', '', '')
		ELSE
			INSERT INTO point (uid,aid,time,zoneid,zonelocalid,accountstart,lastlogin,enddate) VALUES (@uid,@aid,@remain, 0, 0, @now, '', '')
	END

	IF @type = 0
	BEGIN
		UPDATE point SET accountstart = @now WHERE @uid = uid AND @aid = aid
	END
	ELSE IF @type = 1
	BEGIN
		SELECT @timeused = datediff(second, @accountstart, @now)
		IF @timeused > 10
		BEGIN
			IF @enddate = NULL OR '2005-01-01' > @enddate
				SELECT @timeminus = @timeused
			ELSE IF @now > @enddate AND datediff(second,@enddate,@now) > @timeused
				SELECT @timeminus = @timeused
			ELSE IF @now > @enddate AND @timeused > datediff(second,@enddate,@now)
				SELECT @timeminus = datediff(second,@enddate,@now)
			IF @timeminus > @remain	SELECT @timeminus = @remain
			IF @timeminus > 3600 SELECT @timeminus = 0
			UPDATE point SET time=time-@timeminus, accountstart='' WHERE @uid=uid AND @aid=aid
		END
	END
	ELSE IF @type = 2
	BEGIN
		SELECT @timeused = datediff(second, @accountstart, @now)
		IF @timeused > 10
		BEGIN
			IF @enddate = NULL OR '2005-01-01' > @enddate
				SELECT @timeminus = @timeused
			ELSE IF @now > @enddate AND datediff(second,@enddate,@now) > @timeused
				SELECT @timeminus = @timeused
			ELSE IF @now > @enddate AND @timeused > datediff(second,@enddate,@now)
				SELECT @timeminus = datediff(second,@enddate,@now)
			IF @timeminus > @remain	SELECT @timeminus = @remain
			IF @timeminus > 3600 SELECT @timeminus = 300
			UPDATE point SET time=time-@timeminus, accountstart=@now WHERE @uid=uid AND @aid=aid
		END
	END

	IF @timeused > 10
	BEGIN
		UPDATE function SET adduptime = ISNULL(adduptime,0) + @timeused WHERE @uid = uid
		IF @@rowcount = 0	INSERT INTO function (uid,adduptime) VALUES (@uid, @timeused)
	END
	COMMIT TRAN
</procedure>

