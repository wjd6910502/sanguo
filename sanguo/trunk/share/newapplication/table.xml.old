<procedure name="bindmatrix" connection="auth0" operate="replaceA">
	<!-- -1.unknown error;  -2.userid invalid; -3. coordinates invalid; -4. verifyCode error;
	 -5.matrix id invalid;	 -6.already bind; -7.too many illegal try; -8. bind related method is in process;
	  -9.bind mobile; -10.matrix not be audited. -11.matrix expired  -->
	<parameter name="userid"        sql-type="integer"        java-type="java.lang.Integer" in="true"  out="false" />
	<parameter name="matrixid"      sql-type="varchar(12)"    java-type="java.lang.String"  in="true"  out="false" />
	<parameter name="ip"            sql-type="integer"        java-type="java.lang.Integer" in="true"  out="false" />
	BEGIN TRAN
	    DECLARE @newmatrixFromTable       varbinary(80)
	    DECLARE @recordidFromTable        integer
	    DECLARE @statusFromTable          integer
	    DECLARE @expiredDateFromTable     datetime
   	    DECLARE @bindstatusFromTable      integer
	    
	    SELECT id FROM users where id = @userid
		IF (@@rowcount = 0)
		BEGIN
			rollback tran
			return -2 <!--userid invalid-->
		END
	    
	    SELECT @recordidFromTable=recordid ,@newmatrixFromTable=matrix FROM matrixnew where id = @matrixid at isolation read uncommitted
		IF (@@rowcount = 0)
		BEGIN
			rollback tran
			return -5 <!--not exist -->
		END
		
		SELECT @statusFromTable=status, @expiredDateFromTable = expiredtime FROM matrixrecord where id = @recordidFromTable
		IF (@@rowcount = 1)
		BEGIN
			IF (@statusFromTable != 1)
		    BEGIN
				rollback tran
				return -10 <!-- not be audited -->
			END	
			IF (getdate() > @expiredDateFromTable)
		    BEGIN
				rollback tran
				return -11  <!-- expired -->
			END	
		END ELSE
		BEGIN
			rollback tran
			return -5 <!-- invalid -->
		END
		
		SELECT @bindstatusFromTable = bindstatus from iplimit WHERE uid = @userid 
		IF (@@rowcount = 1)
		BEGIN
			IF (@bindstatusFromTable > 1)
		    BEGIN
				rollback tran
				return -9 <!-- bind mobile -->
			END	
			IF (@bindstatusFromTable = 1)
		    BEGIN
				rollback tran
				return -6 <!-- already bind matrix-->
			END	
			UPDATE iplimit SET bindstatus = 1, recordid =@recordidFromTable, matrixid = @matrixid, matrix=@newmatrixFromTable WHERE uid = @userid    		
		END
		ELSE BEGIN
			INSERT INTO iplimit (uid,enable,lockstatus,autolock, bindstatus, recordid,matrixid,matrix) VALUES (@userid,'f','f','f',1,@recordidFromTable,@matrixid,@newmatrixFromTable)		
		END		
	
		INSERT INTO matrixused (id,matrix,userid, begindate,ip) VALUES (@matrixid,@newmatrixFromTable,@userid,getdate(), @ip)		
		DELETE FROM matrixnew where id = @matrixid

	COMMIT TRAN
</procedure>

<procedure name="unbindmatrix" connection="auth0" operate="replaceA">
	<!-- -1.unknown error;  -2.userid invalid; -3. coordinates invalid; -4. verifyCode error; -5.matrix id wrong; -6.no bind; -7.too many illegal try; -8. bind related method is in process -->
	<parameter name="userid"        sql-type="integer"     java-type="java.lang.Integer" in="true"  out="false" />
	<parameter name="matrixid"      sql-type="varchar(12)"    java-type="java.lang.String"  in="true"  out="false" />
	<parameter name="ip"            sql-type="integer"     java-type="java.lang.Integer" in="true"  out="false" />

    DECLARE @matrixidFromTable     varchar(12)
    DECLARE @statusFromTable       integer
	    
    SELECT @statusFromTable=bindstatus, @matrixidFromTable = matrixid FROM iplimit WHERE uid = @userid
    IF (@@rowcount = 1)
    BEGIN
    	IF (@statusFromTable != 1)
    	BEGIN
    		return 0
    	END
    	<!--
    	IF ( @matrixidFromTable != @matrixid )
    	BEGIN
    		return -5
    	END
    	-->
    	BEGIN TRAN    
			UPDATE iplimit SET bindstatus = 0, matrixid = NULL WHERE uid = @userid    		
		    UPDATE matrixused SET canceldate = getdate(),ip=@ip WHERE id = @matrixidFromTable    		
		COMMIT TRAN
    END 
</procedure>


